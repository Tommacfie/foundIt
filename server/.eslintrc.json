{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "prefer-destructuring": "off",
    "consistent-return": "off",
    "object-shorthand": "off",
    "no-restricted-syntax": "off"
  }
}
