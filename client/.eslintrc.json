{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "prefer-destructuring": "off",
    "object-shorthand": "off",
    "no-restricted-syntax": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
